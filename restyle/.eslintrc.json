{
  "extends": "airbnb-base",
  "rules": {
    "no-console": "off",
    "import/no-extraneous-dependencies": "off",
    "no-underscore-dangle": "off",
    "consistent-return": "off",
    "camelcase": "off",
    "no-unused-vars": ["error", { "argsIgnorePattern": "next" }]
  },
  "env": {
    "node": true,
    "jest": true
  }
}